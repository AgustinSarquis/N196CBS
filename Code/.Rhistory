# Pakukui growth model from Lincoln 2023
#
kukui_model <- function(t, density) {
DBH <- -9.021135 + 16.757157 * log(t) # from Lincoln 2023
HEIGHT <- -3.635323 + 8.5568025 * log(t) # from Lincoln 2023
VOLUME <- exp(-4.41727)*DBH^2.034754*HEIGHT^0.85938
BIOMASS <- VOLUME * density
return(data.frame(
t = t,
DBH = DBH,
HEIGHT = HEIGHT,
VOLUME = VOLUME,
BIOMASS = BIOMASS
))
}
# Create a time series (e.g., from 0 to 100 in steps of 1)
time_series <- seq(0, 135, by = 1)
kukui_values = kukui_model(time_series, 330)
View(kukui_values)
